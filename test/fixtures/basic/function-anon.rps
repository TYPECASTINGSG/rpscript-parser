assign 'filename' "api-list.txt"
assign 'sortedfilename' "sorted-api-list.txt"

console-log $filename
@writeSortedApis $filename $sortedfilename

@writeSortedApis $readFile $writeFile {
    console-log $readFile
    ;read $readFile
    ;lodash "split" $RESULT "\n"
    ;write $writeFile columnify lodash "orderBy" $RESULT
}


;@generateApis $filename

@generateApis $filename{
    glob "rpscript-api-*"
    map $RESULT @ $item { replace-content $item "rpscript-api-" "" }
    write $filename columnify chunk $RESULT 1
}
